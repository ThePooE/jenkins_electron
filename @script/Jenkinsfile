#!groovy
import hudson.model.*
import hudson.EnvVars
import groovy.json.JsonSlurper
import groovy.json.JsonBuilder
import groovy.json.JsonOutput
import java.net.URL

node {
    def slurper = new JsonSlurper()
    stage ('Download') {
        checkout scm
        echo 'Yay!'
    }
    stage ('Setup') {
        bat 'npm run package-properties'
        String properties = readFile("package-properties.txt")
        withEnv(properties) {
            echo currentJob.getName()
            echo properties
        }
    }
}