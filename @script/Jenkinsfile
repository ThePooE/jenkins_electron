#!groovy
node {
    stage ('Download') {
        checkout scm
        echo 'Yay!'
    }
    stage ('Setup') {
        bat 'npm run package-properties'
        echo '${env.BUILD_ID}'
        bat 'npm install'
    }
    state ('Test') {
        bat 'npn run e2e'
        bat 'npn run coverage'
        bat 'npn run packWin'
    }
}